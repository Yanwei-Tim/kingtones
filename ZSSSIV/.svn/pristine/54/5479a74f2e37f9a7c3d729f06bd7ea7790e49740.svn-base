package com.kingtone.jw.service.jwoa.bean;

/**
 * 收文情况记录
 * @author     litengmin
 * @since      [产品/模块版本] （必须）
 * @date 2013-8-10
 */

public class ReceiptFileInfo {
    private String receiptName;//收文人 
    private String fileCode;//文件号 
    private String depName;//部门名称 
    private String receiptStatus;//收文状态 
    private String receiptStatus2;//收文状态2 
    private String receiptCode;//收文编号 
    private String postSite;// 发送站点 
    private String guId;// GUID 
    private String receiptSite;// 接收站点 
    private String tr;// TR 
    private String signDate;// 签收时间 
    public String getReceiptName() {
        return receiptName;
    }
    public void setReceiptName(String receiptName) {
        this.receiptName = receiptName;
    }
    public String getFileCode() {
        return fileCode;
    }
    public void setFileCode(String fileCode) {
        this.fileCode = fileCode;
    }
    public String getDepName() {
        return depName;
    }
    public void setDepName(String depName) {
        this.depName = depName;
    }
    public String getReceiptStatus() {
        return receiptStatus;
    }
    public void setReceiptStatus(String receiptStatus) {
        this.receiptStatus = receiptStatus;
    }
    public String getReceiptStatus2() {
        return receiptStatus2;
    }
    public void setReceiptStatus2(String receiptStatus2) {
        this.receiptStatus2 = receiptStatus2;
    }
    public String getReceiptCode() {
        return receiptCode;
    }
    public void setReceiptCode(String receiptCode) {
        this.receiptCode = receiptCode;
    }
    public String getPostSite() {
        return postSite;
    }
    public void setPostSite(String postSite) {
        this.postSite = postSite;
    }
    public String getGuId() {
        return guId;
    }
    public void setGuId(String guId) {
        this.guId = guId;
    }
    public String getReceiptSite() {
        return receiptSite;
    }
    public void setReceiptSite(String receiptSite) {
        this.receiptSite = receiptSite;
    }
    public String getTr() {
        return tr;
    }
    public void setTr(String tr) {
        this.tr = tr;
    }
    public String getSignDate() {
        return signDate;
    }
    public void setSignDate(String signDate) {
        this.signDate = signDate;
    }
}
